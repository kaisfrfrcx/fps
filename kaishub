-- Whitelist of usernames 
local whitelist = {
    "kaisfrms2hub",  
    "LAC_cal69",
    "Mr_ladu", 
    "legendgroupholder299",
    "Player",
    "Player",
    "Player",
    "Player",
    "Player",
    "Player",
