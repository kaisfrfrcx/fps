    for i,v in pairs (game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character.Head:FindFirstChild("Esp Sht")==nil  then 
            esp.Text = "{"..v.Name.."}"
            gui:Clone().Parent = v.Character.Head
        end
    end
end)     
   end,
})

